{
    "name": "@thangdevalone/meet-layout-grid-vue",
    "version": "1.0.1",
    "description": "Vue 3 integration for meet-layout-grid with Motion animations",
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.mts",
                "default": "./dist/index.mjs"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            }
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "unbuild",
        "dev": "unbuild --watch",
        "clean": "rimraf dist",
        "test": "vitest run",
        "test:watch": "vitest"
    },
    "keywords": [
        "vue",
        "vue3",
        "grid",
        "meeting",
        "video",
        "motion",
        "animation"
    ],
    "author": "ThangDevAlone",
    "license": "MIT",
    "homepage": "https://github.com/thangdevalone/meet-layout-grid#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/thangdevalone/meet-layout-grid"
    },
    "peerDependencies": {
        "vue": ">=3.3.0"
    },
    "dependencies": {
        "@thangdevalone/meet-layout-grid-core": "workspace:*",
        "@vueuse/core": "^10.7.0",
        "motion-v": "^1.0.0"
    },
    "devDependencies": {
        "vue": "^3.4.0",
        "unbuild": "^2.0.0",
        "vitest": "^1.0.0",
        "rimraf": "^5.0.0"
    }
}
