{
    "name": "@thangdevalone/meet-layout-grid-react",
    "version": "1.3.2",
    "description": "React integration for meet-layout-grid with Motion animations",
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.mts",
                "default": "./dist/index.mjs"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            }
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "unbuild",
        "dev": "unbuild --watch",
        "clean": "rimraf dist",
        "test": "vitest run",
        "test:watch": "vitest"
    },
    "keywords": [
        "react",
        "grid",
        "meeting",
        "video",
        "motion",
        "animation"
    ],
    "author": "ThangDevAlone",
    "license": "MIT",
    "homepage": "https://github.com/thangdevalone/meet-layout-grid#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/thangdevalone/meet-layout-grid"
    },
    "peerDependencies": {
        "react": ">=18.0.0",
        "react-dom": ">=18.0.0"
    },
    "dependencies": {
        "@thangdevalone/meet-layout-grid-core": "workspace:*",
        "motion": "^11.15.0"
    },
    "devDependencies": {
        "@types/react": "^18.2.0",
        "@types/react-dom": "^18.2.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "unbuild": "^2.0.0",
        "vitest": "^1.0.0",
        "rimraf": "^5.0.0"
    }
}
