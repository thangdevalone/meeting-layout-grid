{
    "name": "@meet-layout-grid/angular",
    "version": "1.0.0",
    "description": "Angular integration for meet-layout-grid with Motion animations",
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.mts",
                "default": "./dist/index.mjs"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            }
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "unbuild",
        "dev": "unbuild --watch",
        "clean": "rimraf dist",
        "test": "vitest run"
    },
    "keywords": [
        "angular",
        "grid",
        "meeting",
        "video",
        "motion",
        "animation"
    ],
    "author": "ThangDevAlone",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/thangdevalone/meet-layout-grid"
    },
    "peerDependencies": {
        "@angular/core": ">=17.0.0",
        "@angular/common": ">=17.0.0"
    },
    "dependencies": {
        "@meet-layout-grid/core": "workspace:*",
        "motion": "^11.15.0"
    },
    "devDependencies": {
        "@angular/core": "^17.0.0",
        "@angular/common": "^17.0.0",
        "rxjs": "^7.8.0",
        "unbuild": "^2.0.0",
        "vitest": "^1.0.0",
        "rimraf": "^5.0.0",
        "zone.js": "^0.14.0"
    }
}