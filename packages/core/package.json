{
    "name": "@thangdevalone/meet-layout-grid-core",
    "version": "1.0.2",
    "description": "Core grid calculation logic for meet-layout-grid",
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.mts",
                "default": "./dist/index.mjs"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            }
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "unbuild",
        "dev": "unbuild --watch",
        "clean": "rimraf dist",
        "test": "vitest run",
        "test:watch": "vitest"
    },
    "keywords": [
        "grid",
        "responsive",
        "meeting",
        "video",
        "layout"
    ],
    "author": "ThangDevAlone",
    "license": "MIT",
    "homepage": "https://github.com/thangdevalone/meet-layout-grid#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/thangdevalone/meet-layout-grid"
    },
    "devDependencies": {
        "unbuild": "^2.0.0",
        "vitest": "^1.0.0",
        "rimraf": "^5.0.0"
    }
}
